# Odoo Manager Instances Configuration
# This file contains configuration for all Odoo instances

instances:
  # Example instance configuration
  production:
    version: "17.0"
    edition: community
    deployment_type: docker
    port: 8069
    workers: 4
    max_cron_threads: 2
    db_maxconn: 64
    db_name: prod
    db_host: localhost
    db_port: 5432
    db_user: odoo
    db_password: odoo
    db_filter: "^prod$"
    addons_path: null
    without_demo: true
    admin_password: admin
    image: null
    postgres_image: postgres:15
    env_vars: {}
